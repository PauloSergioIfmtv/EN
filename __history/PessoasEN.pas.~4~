unit PessoasEN;

interface

uses StrUtils, SqlTimSt, ExtCtrls, Classes;

type
  TPessoas = class
  private
    { Private declarations }
    psNome: String;
  public
    { Public declarations }
    constructor Create;
    destructor Destroy; override;
    property sNome : String read psNome write psNome;
  end;

implementation

{ TPessoas }

constructor TPessoas.Create;
begin
  inherited Create;
end;

destructor TPessoas.Destroy;
begin
  inherited Destroy;
end;

end.
